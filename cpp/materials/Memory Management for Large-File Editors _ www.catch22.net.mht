/* $Id: style.css,v 1.38.2.1 2008/02/05 09:27:26 goba Exp $ */

/**
 * Garland, for Drupal 6.x
 * Stefan Nagtegaal, iStyledThis [dot] nl
 * Steven Wittens, acko [dot] net`
 *
 * If you use a customized color scheme, you must regenerate it after
 * modifying this file.
 */

#primary-block {
 	position: relative;
	width: 100%;
	overflow: hidden;
	display: block;

	margin:0;
	padding:0;
	border:0;
	font-size: 50%;

	font-family: Helvetica, sans-serif;
	zoom: 1;
}

ul.primary-links {
	list-style: none;
	display: block;
	width: 100%;
	margin:0;
	padding:0;
	border:0;
}



ul.primary-links li {
	list-style: none;
	float: left;
	padding: 0;
	margin: 0 2px;   /* was: 0 10px */
	border: 0;
/*	display: inline;*/
}

ul.primary-links {
/*	display: inline;*/
 	font-size: 11pt;
	font-weight: bold;
}

ul.primary-links li.active,
ul.primary-links li.active-trail {
	  background-image: url('images/tabright.gif');
	  background-repeat: no-repeat;
 	 background-position: top right;
 
	 margin: 0 0px;

	 /*height: 50px;*/
}



ul.primary-links li a, 
ul.primary-links li a:visited,
ul.primary-links li a:hover { 
	float: none;
	display: block;
	height: 50px;

	border: 0;
	margin: 0;
	padding: 0 8px;   /* was: 0 0px */
	text-decoration: none;
	/*padding-bottom: 20px;*/
	color: #2C4A73;

	padding-top: 12px;
/*	line-height: 50px;*/

}


ul.primary-links li a:hover {
	background-image: url('images/selback3.gif');
	background-repeat: repeat-x;
/*	background-position: center 8px;*/
	color:white; /*#4A83B3;*/
} 

ul.primary-links li.active a,
ul.primary-links li.active-trail a {
  background-image: url('images/tableft.gif');
  background-repeat: no-repeat;
  background-position: top left;
  color: white;
  padding-left: 30px;
  padding-right: 30px;
  overflow: visible;
}


#secondary-block {
 margin: 0 0;
 position: relative;
 clear: both;
 float: right;
 display: block;
 background-image: url('images/roundleft.gif');
 background-repeat: no-repeat;
 background-color: #4A83B3;
/* background-position: top right;*/
 padding-left: 16px;

}

#secondary-block ul.links {
 border: 0;
 margin: 0;
	list-style: none;
	display: block;

 padding: 4px 4px;
 padding-bottom: 0px;
 padding-left: 16px;
 text-align: left;
}

#secondary-block ul.links a {
 color: white;
 font-weight: bold;
}

#secondary-block ul.links li.active a,
#secondary-block ul.links li.active-trail a {
 text-decoration: underline;
}

#tertiary-block {
 display: block;
 position: relative; 
 border: 0;
 padding: 0;
 margin: 0;
 overflow: hidden;
}

#tertiary-block ul.links {
/* display: inline;*/
 list-style: none;
/* display: block;*/

}

#tertiary-block ul.links li {
 padding: 5px 8px;
 margin:  0 8px;
 list-style: none;
/* display: inline;*/
} 

#tertiary-block ul.links li.first {
 margin: 0 0;
}

#tertiary-block ul.links li.last {

}

#tertiary-block ul.links li.active {
 background-color: #9abbd6;
 font-weight: bold;
}

#tertiary-block ul.links li a {
 color: #2E4D78;
 font-size: 1.2em;
 line-height: 1.8em;
}
 

#tertiary-block ul.links li.active a {
 color: white;
}



/**
 * Generic elements
 */

#primary-tabs {
  background-color: #C7DFF0; /*#A4CBE6;*/
/*  margin-top: -26px;*/

  padding-top: 8px;
  font-size: 1.5em;
  overflow: visible;
}

#primary-tabs ul {
}

#primary-tabs ul li.active,
#primary-tabs ul li.active-trail  {
  background-color: white;
  background-image: url('images/tabback.gif');
  background-position: top right;
}

#primary-tabs ul li a {
   padding: 8px;
   padding-top: 2px;
}


#secondary-tabs {
  background-color: white;
  margin-bottom: 32px;
  margin-top: 4px;
}


/**
 * Local tasks
 */
ul.primary, ul.primary li, ul.secondary, ul.secondary li {
  border: 0;
  background: none;
  margin: 0;
  padding: 0;
}

#tabs-wrapper {
 /* margin: 0 -26px 1em;*/
/*  padding: 0 26px;*/
/*  border-bottom: 1px solid #e9eff3;*/
  position: relative;

  margin: 0;
  padding: 0;
    height: 20px;
  line-height: 20px;
}
ul.primary {
/*  padding: 0.5em 0 10px;*/
  padding: 0;
  margin: 0;

  float: left; /* LTR */
}
ul.secondary {
  clear: both;
  text-align: left; /* LTR */
  border-bottom: 1px solid #e9eff3;
  margin: -0.2em -26px 1em;
  padding: 0 26px 0.6em;
}


h2.with-tabs {
/*  float: left;   */     /* LTR */
  margin: 0 2em 0 0; /* LTR */
  padding: 0;
}

ul.primary {
 float: right;
/*  margin-right: 36px*/
}

ul.primary li {
 margin: 0;
