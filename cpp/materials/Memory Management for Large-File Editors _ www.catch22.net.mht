#container-body {
 margin: 0;
 padding: 0;
 border: 0;
/* width: 800px;*/

  background-image: url('images/pageback2.gif');
  background-repeat: repeat-x;
  background-color: #4A7EB3; /*#4A83B3;*/

}

body.with-left-sidebar  #container-body,
body.with-right-sidebar #container-body {
 width: 1076px;
}

body.with-both-sidebars  #container-body {
 width: 1352px;
}

#top {
 height: 4px;
 background-color: #2C4A73;
 clear: both;
}

#left-container {
  position: relative;
  width: 100%;
  height: 100%;
}

#left-sidebar {
  float: left;
  width: 276px;
  position: relative;
  height: 100%;

  background-image: url('images/logo.gif');
  background-repeat: no-repeat;
  background-position: top right;
  padding: 0;
  margin: 0;
  padding-top: 200px;
}

#left-sidebar .block {
  color: white;
}

#right-sidebar {
  float: left;
  width: 276px;
  position: relative;
}

#main {
  position: relative;
/*  margin-left: 226px;  */
  width: 800px;
  float: left;
}




#left-content { 
  position: relative;
  float: left;
  width: 50%;
}

#right-content { 
  position: relative;
  float: right;
  width: 50%;
}



#rightborder {
   background-image: url('images/backright1.gif');
   background-repeat: repeat-y;
   background-position: top right;
   position: relative;
}


#leftborder {
   background-image: url('images/backleft1.gif');
   background-repeat: repeat-y;
   background-position: top left;
   position: relative;
}


#leftborder2 {
   background-image: url('images/backleft2.gif');
   background-repeat: no-repeat; 
   background-position: top left;
}

#rightborder2 {
   background-image: url('images/backright2.gif');
   background-repeat: no-repeat; 
   background-position: top right;
   padding: 0 8px;
}


