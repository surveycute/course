        <span class=3D"comment">// normal text colours</span>
        else
        {
           attr[i].fg =3D GetColour(TXC_FOREGROUND);
           attr[i].bg =3D GetColour(TXC_BACKGROUND);
        }

<p>        attr[i].style =3D 0;<br />
