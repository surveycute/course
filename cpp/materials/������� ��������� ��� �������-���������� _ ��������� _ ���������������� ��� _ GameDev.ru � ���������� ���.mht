body {background: #f6f9ff; margin: 0em; padding: 0px;}

div {color: #000000; font-family: verdana, arial, sans-serif; font-size: 12px; padding: 0px; margin: 0px}

#all {background: #FFFFFF; margin: auto; padding: 2px;}
#path     { background: #CFCFCF; margin-top: 5px; margin-bottom: 5px; padding-top: 1px; min-height: 19px;}
#header   { }
#container{ background: #E7EDF3; width: 100%;}
#left     { width: 120px; margin: 3px; float: left}
#right    { float: right; margin-top: 1px;}
#footer   { background: #CFCFCF; margin-top: 5px; margin-bottom: 5px; padding: 2px; text-align: center;}
#bottom   { background: #FFFFFF; border: 0; text-align:center;}
#main     { background: #FFFFFF; width: auto; min-height: 200px; margin-left: 125px; padding-left: 5px; padding-right: 5px; padding-bottom: 20px;}

div.overright {overflow: auto; max-height: 360px;}
div.block{border: 0px; padding: 0px; margin-left: 5px; margin-right: 2px; margin-top: 10px; margin-bottom: 2px;}
div.text {padding-top: 2px; padding-left: 6px; padding-right: 6px; padding-bottom: 10px;}
div.mes {border: 1px solid #c5d5f0; padding: 1px; margin-left: 0px; margin-right: 0px; margin-top: 2px; margin-bottom: 10px; height: 1%;}
div.docs {border: 1px solid #c5d5f0; padding: 3px; margin-left: 0px; margin-right: 0px; margin-top: 2px; margin-bottom: 20px;}
div.clear {clear: both}

div.title {color: #00304b; background: #aecccc; font-size: 17px; padding: 6px; margin-bottom: 4px}
div.code {background: #e8e8e8; }
div.quote {background: #EFEFEF; border: 2px solid #ffffff; margin-left: 25px; margin-right: 25px; padding: 5px}
blockquote {background: #EFEFEF; border: 2px solid #ffffff; margin-left: 25px; margin-right: 25px; padding: 5px}
div.offtop {border: 2px solid #ffffff; color: #808080; margin-left: 10px; margin-right: 25px; padding: 5px; font-size: 10px; }

div.menu {font-size: 12px; border: 1px solid #a6bfdc; padding: 1px; margin-bottom: 10px}
div.menu p {background: #92B4D7; font-weight: bold; min-height: 15px; font-size: 12px; margin: 1px; padding: 2px}

div.submenu {padding: 0px; margin: 0px}
div.submenu p {background: #CDD5DF; font-weight: normal; min-height: 15px; font-size: 12px; margin: 1px; padding: 2px}

div.list {background: #D5E0E0; color: #808080; min-height: 15px; font-size: 11px; border: 1px; margin: 1px; padding: 3px}
div.list b {font-size: 12px;}

div.menuitem {background: #92B4D7; font-weight: bold; min-height: 15px; font-size: 12px; border: 1px; margin: 1px; padding: 2px}
div.menusel  {background: #cDd5df; font-weight: bold; height: 15px; font-size: 12px; border: 1px; margin: 1px; padding: 1px}
div.menucode {background: #CDD5DF; min-height: 15px; font-size: 11px; border: 1px; margin: 1px; padding: 1px}
div.menuco   {background: #D5E0E0; min-height: 15px; font-size: 11px; border: 1px; margin: 1px; padding: 1px}
div.doccode {background: #EFEFEF; padding: 3px; margin-left: 0px; margin-right: 0px; margin-top: 2px; margin-bottom: 2px;}
div.docco   {background: #E7EDF3; padding: 3px; margin-left: 0px; margin-right: 0px; margin-top: 2px; margin-bottom: 2px;}

div.menu p a:link, div.menu p a:visited, div.menu p a:active {color: #00002A; text-decoration: none}
div.menu p a:hover {color: #FFFFFF; text-decoration: none}

#headertext {padding: 1px; font-style:italic; font-weight: bold; font-size: 46px; font-family: impact, sans-serif;}
#headertext a {text-decoration: none;}

p {margin-top: 10px; margin-bottom: 10px;}
p.r {text-align: right;}
p.mar {text-indent: 2em;}

span.q    {color: #808080;}
span.red  {color: #FF0000;}
span.blue {color: #0000FF;}
span.green{color: #00FF00;}
span.r {float:right;}

a:link    {color: #2E2E5C;}
a:visited {color: #318DC4;}
a:active  {color: #00304B;}
a:hover   {color: #BF3A00;}

table.code {padding: 0; margin: 0;}

table.r {border: #c5d5f0 1px solid; width: 100%; }
table.r th {background: #a6bfdc; padding: 3px; text-align: left; font-weight: bold; font-size: 12px;}
table.r td {background: #D5E0E0; padding: 3px; }
table.r tr.sec td {background: #CDD5DF;}
table.r tr.red td {background: #E5D0D0;}

table.mes {width: 100%;}
table.mes th {background: #D5E0E0; text-align: left; width: auto; padding-left: 3px;}
table.mes th b {font-size: 17px;}
table.mes th.red {background: #E5D0D0;}
table.mes td {background: #CDD5DF; color: #808080; text-align: right; width: 45px; padding: 2px;}

td {font-family: verdana, arial, sans-serif; font-size: 12px; vertical-align: middle}
td.co   {background: #D5E0E0;}
td.code   {background: #CDD5DF;}

b.title  {color: #00304b; font-size: 17px;}

h1 {color: #00304b; font-size: 21px; margin: 0;}
h2 {color: #00304b; font-size: 18px; margin: 0;}
h3 {color: #00304b; font-size: 14px; margin: 0;}

pre {font-size: 14px; font-family: 'courier new',courier, monospace; text-align: left; margin: 0px}

.thinbut{border: #c5d5f0 1px solid; font-size: 11px; font-style: normal; color: #000000; background: #ffffff; padding-top: 0px; padding-bottom: 0px; margin-top: 0px; margin-bottom: 0px;}

img {border: 0}
img.tex {vertical-align: middle}

form {margin: 0;}
textarea.gdr {color: #000000; font-family: verdana, arial, sans-serif; font-size: 12px; width: 99%; background: #FFFFFF;}
input {font-family: verdana, arial, sans-serif; font-size: 12px; background: #FFFFFF; }

ul {padding-left: 15px; margin: 2px}
li {padding-left: 0px; padding-top: 3px; padding-right: 0px; padding-bottom: 5px; margin-left: 15px; margin-top: 0px; margin-right: 15px; margin-bottom: 0px;}

div.seo {color: #808080; font-size: 10px;}
div.seo a:link    {color: #808080; text-decoration: none}
div.seo a:visited {color: #808080; text-decoration: none}
div.seo a:active  {color: #808080; text-decoration: none}
div.seo a:hover   {color: #808080; text-decoration: none}
