						<!-- firstHeading -->
			<h1 id=3D"firstHeading" class=3D"firstHeading">Stack buffer overflow<=
/h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id=3D"bodyContent">
				<!-- tagline -->
				<div id=3D"siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
				<!-- subtitle -->
				<div id=3D"contentSub"></div>
				<!-- /subtitle -->
