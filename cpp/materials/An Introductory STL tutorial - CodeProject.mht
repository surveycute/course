	{
		<span class=3D"code-keyword">if</span> ( !lock())
			<span class=3D"code-keyword">return</span>;
		vector<span class=3D"code-keyword">&lt;</span>T<span class=3D"code-key=
word">&gt;</span>::push_back (obj);
		unlock();
	}
};</pre>
